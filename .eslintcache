"72"],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Checkout/Checkout.js",["73"],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Home/Home.js",[],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/DataLayer/StateProvider.js",[],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Product/Product.js",["74"],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/DataLayer/reducer.js",[],["75","76"],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Header/Header.js",["77"],"import React from 'react';\nimport './Header.css'\nimport SearchIcon from \"@material-ui/icons/Search\";\nimport ShoppingBasketIcon from \"@material-ui/icons/ShoppingBasket\";\nimport { Link } from \"react-router-dom\";\nimport { useStateValue } from \"../DataLayer/StateProvider\";\nimport { auth } from '../firebase'\n\nexport default function Header() {\n    const [{ basket, user }, dispatch] = useStateValue();\n    const signOut = () => {\n        // [START auth_sign_out]\n        auth.signOut().then(() => {\n            // Sign-out successful.\n            dispatch({\n                type: \"SET_USER\",\n                user: null\n            })\n        }).catch((error) => {\n            // An error happened.\n        });\n        // [END auth_sign_out]\n    }\n\n    return (\n\n        <div className=\"header\">\n            <Link to=\"/\">\n\n                <img\n                    className=\"header__logo\"\n                    src=\"http://pngimg.com/uploads/amazon/amazon_PNG11.png\"\n                />\n            </Link>\n            <div className=\"header__search\">\n                <input className=\"header__searchInput\" type=\"text\" />\n                <SearchIcon className=\"header__searchIcon\" />\n            </div>\n            <div className=\"header__nav\">\n                <Link to={!user && '/login'}>\n                    <div className=\"header__option\">\n                        <span className=\"header__optionLineOne\">Hello {user ? user.email : 'Guest'}</span>\n                        <span className=\"header__optionLineTwo\">{user ? <span onClick={signOut}>SignOut</span> : 'SignIn'}</span>\n                    </div>\n                </Link>\n\n                <div className=\"header__option\">\n                    <span className=\"header__optionLineOne\">Returns</span>\n                    <span className=\"header__optionLineTwo\">& Orders</span>\n                </div>\n\n                <div className=\"header__option\">\n                    <span className=\"header__optionLineOne\">Your</span>\n                    <span className=\"header__optionLineTwo\">Prime</span>\n                </div>\n\n\n                <Link to=\"/checkout\">\n\n                    <div className=\"header__optionBasket\">\n                        <ShoppingBasketIcon />\n                        <span className=\"header__optionLineTwo header__basketCount\">\n                            {basket?.length}\n                        </span>\n                    </div>\n\n\n                </Link >\n\n            </div>\n\n        </div>\n    )\n}\n","/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Checkout/CheckoutProduct/CheckoutProduct.js",["78","79"],"import React from 'react';\nimport './CheckoutProduct.css'\nimport { useStateValue } from \"../../DataLayer/StateProvider\";\n\n\nexport default function CheckoutProduct({ id, image, title, price, rating }) {\n\n    const [{ basket }, dispatch] = useStateValue();\n\n    const removeFromBasket = () => {\n        // remove the item from the basket\n        dispatch({\n            type: 'REMOVE_FROM_BASKET',\n            id: id,\n        })\n    }\n\n    return (\n        <div className=\"checkoutProduct\">\n\n            <img className='checkoutProduct__image' src={image} />\n\n            <div className='checkoutProduct__info'>\n                <p className='checkoutProduct__title'>{title}</p>\n                <p className=\"checkoutProduct__price\">\n                    <small>$</small>\n                    <strong>{price}</strong>\n                </p>\n                <div className=\"checkoutProduct__rating\">\n                    {Array(rating)\n                        .fill()\n                        .map((_, i) => (\n                            <p>🌟</p>\n                        ))}\n                </div>\n                <button onClick={removeFromBasket}>Remove from Basket</button>\n            </div>\n        </div>\n    )\n}\n","/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Checkout/Subtotal/Subtotal.js",["80"],"import React from 'react'\nimport { useStateValue } from \"../../DataLayer/StateProvider\";\nimport CurrencyFormat from \"react-currency-format\";\nimport { getBasketTotal } from \"../../DataLayer/reducer\";\nimport './Subtotal.css'\n\n\nexport default function Subtotal() {\n    const [{ basket }, dispatch] = useStateValue();\n\n    return (\n        <div className=\"subtotal\">\n            <CurrencyFormat\n                renderText={(value) => (\n                    <>\n                        <p>\n                            Subtotal ({basket.length} items): <strong>{value}</strong>\n                        </p>\n                        <small className=\"subtotal__gift\">\n                            <input type=\"checkbox\" /> This order contains a gift\n            </small>\n                    </>\n                )}\n                decimalScale={2}\n                value={getBasketTotal(basket)}\n                displayType={\"text\"}\n                thousandSeparator={true}\n                prefix={\"$\"}\n            />\n            <button>Proceed to Checkout</button>\n\n        </div>\n    )\n}\n","/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/reportWebVitals.js",[],["81","82"],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Login/Login.js",["83"],"import React, { useState } from 'react';\nimport './Login.css'\nimport { Link, useHistory } from \"react-router-dom\";\nimport { auth } from \"../firebase\";\n\nexport default function Login() {\n    const history = useHistory();\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n\n\n    const signIn = e => {\n        e.preventDefault();\n\n        auth\n            .signInWithEmailAndPassword(email, password)\n            .then(auth => {\n                history.push('/')\n            })\n            .catch(error => alert(error.message))\n    }\n\n\n\n    const register = e => {\n        e.preventDefault();\n\n        auth.createUserWithEmailAndPassword(email, password)\n            .then((auth) => {\n                // if successfully created a new user with email and password \n                if (auth) {\n                    history.push('/');\n                }\n            })\n            .catch(error => alert(error.message));\n\n\n    }\n\n    return (\n        <div className=\"login\">\n            <Link to='/'>\n                <img\n                    className=\"login__logo\"\n                    src='https://upload.wikimedia.org/wikipedia/commons/thumb/a/a9/Amazon_logo.svg/1024px-Amazon_logo.svg.png'\n                />\n            </Link>\n\n            <div className='login__container'>\n                <h1>Sign-in</h1>\n\n                <form>\n                    <h5>E-mail</h5>\n                    <input type='text' value={email} onChange={e => setEmail(e.target.value)} />\n\n                    <h5>Password</h5>\n                    <input type='password' value={password} onChange={e => setPassword(e.target.value)} />\n\n                    <button type='submit' onClick={signIn} className='login__signInButton'>Sign In</button>\n                </form>\n\n                <p>\n                    By signing-in you agree to the AMAZON FAKE CLONE Conditions of Use & Sale. Please\n                    see our Privacy Notice, our Cookies Notice and our Interest-Based Ads Notice.\n                </p>\n\n                <button onClick={register} className='login__registerButton'>Create your Amazon Account</button>\n            </div>\n\n        </div>\n\n\n    )\n}\n","/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/firebase.js",[],"/home/foush/ProjectS 5-8-2020/PROJECTS/JS/Amazon_challenge/amazon-clone/src/Checkout/CheckoutProductNotification/CheckoutProductNotification.js",["84","85"],{"ruleId":"86","severity":1,"message":"87","line":11,"column":10,"nodeType":"88","messageId":"89","endLine":11,"endColumn":13},{"ruleId":"90","severity":1,"message":"91","line":33,"column":6,"nodeType":"92","endLine":33,"endColumn":8,"suggestions":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"94","replacedBy":"98"},{"ruleId":"96","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":10,"column":30,"nodeType":"102","messageId":"103","endLine":10,"endColumn":38},{"ruleId":"100","severity":1,"message":"104","line":11,"column":14,"nodeType":"102","messageId":"103","endLine":11,"endColumn":20},{"ruleId":"94","replacedBy":"105"},{"ruleId":"96","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":30,"column":17,"nodeType":"109","endLine":33,"endColumn":19},{"ruleId":"100","severity":1,"message":"104","line":8,"column":14,"nodeType":"102","messageId":"103","endLine":8,"endColumn":20},{"ruleId":"107","severity":1,"message":"108","line":21,"column":13,"nodeType":"109","endLine":21,"endColumn":67},{"ruleId":"100","severity":1,"message":"101","line":9,"column":24,"nodeType":"102","messageId":"103","endLine":9,"endColumn":32},{"ruleId":"94","replacedBy":"110"},{"ruleId":"96","replacedBy":"111"},{"ruleId":"107","severity":1,"message":"108","line":43,"column":17,"nodeType":"109","endLine":46,"endColumn":19},{"r